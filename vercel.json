{
  "buildCommand": "cd web && pnpm install --no-frozen-lockfile && pnpm run build",
  "outputDirectory": "web/dist",
  "framework": "astro",
  "functions": {
    "/api/room_check": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
